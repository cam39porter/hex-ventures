{
  "name": "client-api",
  "version": "0.0.1",
  "description": "graphql server",
  "main": "index.js",
  "engines": {
    "node": "8.9.4",
    "yarn": "1.5.1"
  },
  "scripts": {
    "test": "jest",
    "compile": "tsc -w",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "build": "tsc",
    "prepush": "yarn build && yarn lint && yarn test",
    "start": "node .compiled/index.js",
    "deploy:api:dev": "gcloud container builds submit --config cloudbuild.yaml . && kubectl replace -f ../ops/dev/client-api/kubernetes/deployment.yaml",
    "deploy:api:prod": "echo 'doesnt work'"
  },
  "author": "",
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(ts)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ]
  },
  "license": "ISC",
  "dependencies": {
    "@google-cloud/language": "1.2.0",
    "@google-cloud/storage": "1.6.0",
