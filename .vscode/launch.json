{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug client-api express server",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/client-api/.compiled/index.js",
      "sourceMaps": true
    },
    {
      "name": "Debug desktop-client main process",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceRoot}/desktop-client/",
      "runtimeExecutable":
        "${workspaceRoot}/desktop-client/node_modules/.bin/electron",
      "windows": {
        "runtimeExecutable":
          "${workspaceRoot}/desktop-client/node_modules/.bin/electron.cmd"
      },
      "args": ["."]
    }
  ]
}
