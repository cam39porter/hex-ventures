{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug client-api express server",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/client-api/.compiled/index.js",
      "sourceMaps": true,
      "envFile": "${workspaceRoot}/client-api/.env",
      "env": { "NODE_ENV": "development" }
    },
    {
      "name": "Launch Chrome against localhost web client",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}/web-client"
    }
  ]
}
