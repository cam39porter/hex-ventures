{
  "name": "web-client",
  "productName": "Tangle",
  "version": "0.1.0",
  "engine": "8.9.4",
  "description":
    "A web client for empowering people to capture and connect thoughts to create big ideas",
  "private": true,
  "dependencies": {
    "apollo-cache-inmemory": "1.1.9",
    "apollo-client": "2.2.5",
    "apollo-link-http": "1.5.2",
    "echarts": "^4.0.4",
    "echarts-for-react": "^2.0.8",
    "graphql": "0.13.0",
    "graphql-tag": "2.8.0",
    "lodash": "^4.17.5",
    "qs": "^6.5.1",
    "react": "^16.2.0",
    "react-apollo": "2.0.4",
    "react-dom": "^16.2.0",
    "react-feather": "^1.0.8",
    "react-quill": "1.2.6",
    "react-router-dom": "^4.2.2",
    "react-scripts-ts": "2.13.0",
    "tachyons": "^4.9.1",
    "tinycolor": "^0.0.1",
    "tinycolor2": "^1.4.1",
    "tinygradient": "^0.3.1"
  },
  "homepage": "./",
  "main": "server.js",
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "schema":
      "apollo-codegen introspect-schema https://client-api-dot-opit-193719.appspot.com/graphql --output ./src/schema.json",
    "types":
      "rm -rf ./src/__generated__ && mkdir ./src/__generated__ && apollo-codegen generate ./src/**/queries.ts --addTypename --schema ./src/schema.json --target typescript --output ./src/__generated__/types.ts",
    "deploy": "yarn build && gcloud -q app deploy ./app.yaml"
  },
  "devDependencies": {
    "@types/echarts": "^0.0.12",
    "@types/graphql": "0.12.4",
    "@types/jest": "^22.1.3",
    "@types/lodash": "^4.14.104",
    "@types/node": "^9.4.6",
    "@types/quill": "^1.3.6",
    "@types/react": "^16.0.38",
    "@types/react-dom": "^16.0.4",
    "@types/react-router": "^4.0.22",
    "@types/react-router-dom": "^4.2.4",
    "apollo-codegen": "0.19.0",
    "typescript": "^2.7.2"
  }
}
