{
  "name": "web-client",
  "productName": "Tangle",
  "version": "0.1.0",
  "description": "A web client for empowering people to capture and connect thoughts to create big ideas",
  "private": true,
  "homepage": "./",
  "main": "server.js",
  "scripts": {
    "start": "REACT_APP_ENV=development react-scripts-ts start",
    "start:local": "REACT_APP_ENV=local react-scripts-ts start",
    "debug": "yarn prestart && REACT_APP_ENV=local react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "analyze": "source-map-explorer build/static/js/main.*",
    "types": "rm -rf src/__generated__/* && apollo client:codegen ./src/__generated__/types.ts --endpoint=http://localhost:8080/graphql --includes=src/**/queries.ts --target=typescript --outputFlat",
    "deploy:web:dev": "REACT_APP_ENV=development yarn build && gcloud -q app deploy ./dev.yaml",
    "deploy:web:prod": "REACT_APP_ENV=production yarn build && gcloud -q app deploy ./prod.yaml",
    "deploy:web:maintenance": "REACT_APP_ENV=production REACT_APP_MAINTENANCE=true yarn build && gcloud -q app deploy ./prod.yaml",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "css": "tachyons src/css/app.css > src/tachyons.css --minify",
    "prestart": "npm run css",
    "prebuild": "npm run css"
  },
  "dependencies": {
    "apollo": "^2.11.0",
    "apollo-cache-inmemory": "1.5.1",
    "apollo-client": "2.5.1",
    "apollo-link-context": "1.0.17",
    "apollo-link-error": "1.1.10",
    "apollo-link-http": "1.5.14",
    "apollo-link-retry": "2.2.13",
    "axios": "0.18.0",
    "draft-convert": "2.1.4",
    "draft-js": "0.10.5",
    "draft-js-hashtag-plugin": "2.0.3",
    "draft-js-inline-toolbar-plugin": "3.0.0",
    "draft-js-linkify-plugin": "2.0.1",
    "draft-js-markdown-shortcuts-plugin": "0.5.0",
    "draft-js-plugins-editor": "2.1.1",
    "echarts": "^4.0.4",
    "echarts-for-react": "^2.0.15-beta.0",
    "firebase": "^6.0.2",
    "graphql": "14.3.0",
    "graphql-tag": "2.10.1",
    "lodash": "^4.17.5",
    "prop-types": "15.7.2",
    "qs": "^6.5.1",
    "react": "^16.2.0",
    "react-apollo": "2.5.5",
    "react-device-detect": "1.6.2",
    "react-dom": "^16.2.0",
    "react-feather": "^1.0.8",
    "react-firebaseui": "^3.1.2",
    "react-ga": "2.5.7",
    "react-resize-detector": "4.1.3",
    "react-router-dom": "^5.0.0",
    "react-scripts-ts": "^3.1.0",
    "react-timeago": "4.4.0",
    "react-toastify": "5.1.0",
    "react-tooltip": "3.10.0",
    "react-window-size": "1.2.2",
    "scroll-into-view": "1.9.7",
    "tachyons": "^4.9.1",
    "typescript": "^3.4.5"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/runtime": "^7.4.4",
    "@firebase/app-types": "^0.4.0",
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-knobs": "5.0.11",
    "@storybook/addon-links": "^5.0.11",
    "@storybook/addon-storyshots": "5.0.11",
    "@storybook/addons": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "@types/axios": "0.14.0",
    "@types/draft-js": "0.10.31",
    "@types/echarts": "^4.1.9",
    "@types/graphql": "14.2.0",
    "@types/jest": "^24.0.12",
    "@types/lodash": "^4.14.104",
    "@types/node": "^12.0.0",
    "@types/react": "^16.0.38",
    "@types/react-dom": "^16.0.4",
    "@types/react-router": "^5.0.0",
    "@types/react-router-dom": "^4.2.4",
    "@types/react-test-renderer": "16.8.1",
    "@types/storybook__addon-actions": "3.4.2",
    "@types/storybook__addon-knobs": "5.0.0",
    "@types/storybook__addon-links": "3.3.4",
    "@types/storybook__addon-storyshots": "4.0.0",
    "@types/storybook__react": "4.0.1",
    "babel-loader": "^8.0.5",
    "react-test-renderer": "16.8.6",
    "source-map-explorer": "1.8.0",
    "tachyons-cli": "1.3.2",
    "ts-loader": "6.0.0"
  }
}
