{
  "name": "web-client",
  "productName": "Tangle",
  "version": "0.1.0",
  "engine": "8.9.4",
  "description": "A web client for empowering people to capture and connect thoughts to create big ideas",
  "private": true,
  "dependencies": {
    "apollo-cache-inmemory": "1.1.9",
    "apollo-client": "2.3.1",
    "apollo-link-context": "1.0.7",
    "apollo-link-error": "1.0.9",
    "apollo-link-http": "1.5.2",
    "apollo-link-retry": "2.2.3",
    "axios": "0.18.0",
    "draft-js": "0.10.5",
    "draft-js-export-html": "1.2.0",
    "echarts": "^4.0.4",
    "echarts-for-react": "^2.0.8",
    "firebase": "4.12.1",
    "graphql": "0.13.0",
    "graphql-tag": "2.8.0",
    "lodash": "^4.17.5",
    "prop-types": "15.6.1",
    "qs": "^6.5.1",
    "react": "^16.2.0",
    "react-apollo": "2.0.4",
    "react-dom": "^16.2.0",
    "react-feather": "^1.0.8",
    "react-firebaseui": "1.2.3",
    "react-router-dom": "^4.2.2",
    "react-scripts-ts": "2.13.0",
    "react-tooltip": "3.5.1",
    "react-window-size": "1.0.1",
    "scroll-into-view": "1.9.3",
    "tachyons": "^4.9.1"
  },
  "homepage": "./",
  "main": "server.js",
  "scripts": {
    "start": "REACT_APP_ENV=development react-scripts-ts start",
    "debug": "REACT_APP_ENV=local react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "schema": "apollo-codegen introspect-schema ../client-api/data-template/schema.graphql --output ./src/schema.json",
    "types": "rm -rf ./src/__generated__ && mkdir ./src/__generated__ && apollo-codegen generate ./src/**/queries.ts --addTypename --schema ./src/schema.json --target typescript --output ./src/__generated__/types.ts",
    "deploy:web:dev": "REACT_APP_ENV=development yarn build && gcloud -q app deploy ./dev.yaml",
    "deploy:web:prod": "REACT_APP_ENV=production yarn build && gcloud -q app deploy ./prod.yaml",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "css": "tachyons src/css/app.css > src/tachyons.css --minify",
    "prestart": "npm run css",
    "prebuild": "npm run css"
  },
  "devDependencies": {
    "@firebase/app-types": "0.1.2",
    "@storybook/addon-actions": "^3.4.3",
    "@storybook/addon-knobs": "3.4.3",
    "@storybook/addon-links": "^3.4.3",
    "@storybook/addon-storyshots": "3.4.3",
    "@storybook/addons": "^3.4.3",
    "@storybook/react": "^3.4.3",
    "@types/axios": "0.14.0",
    "@types/draft-js": "0.10.23",
    "@types/echarts": "^0.0.12",
    "@types/graphql": "0.12.4",
    "@types/jest": "^22.1.3",
    "@types/lodash": "^4.14.104",
    "@types/node": "^9.4.6",
    "@types/react": "^16.0.38",
    "@types/react-dom": "^16.0.4",
    "@types/react-router": "^4.0.22",
    "@types/react-router-dom": "^4.2.4",
    "@types/react-test-renderer": "16.0.1",
    "@types/storybook__addon-actions": "3.0.3",
    "@types/storybook__addon-knobs": "3.3.1",
    "@types/storybook__addon-links": "3.3.0",
    "@types/storybook__addon-storyshots": "3.4.0",
    "@types/storybook__react": "3.0.7",
    "apollo-codegen": "0.19.0",
    "babel-core": "^6.26.3",
    "babel-loader": "7.1.4",
    "babel-runtime": "^6.26.0",
    "react-test-renderer": "16.3.2",
    "tachyons-cli": "1.3.0",
    "ts-loader": "3.5.0",
    "typescript": "^2.7.2"
  }
}
